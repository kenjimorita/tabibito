{
    "version": "1.6.2",
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": false,
        "noImplicitAny": true,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "filesGlob": [
        "./**/*.ts",
        "!./node_modules/**/*.ts"
    ],
    "files": [
        "./controller_ex.ts",
        "./controller_internal.ts",
        "./controllers.ts",
        "./test.ts",
        "./util/strings_external.ts",
        "./util/strings_internal.ts"
    ]
}
